<h2>個人情報登録</h2>

<%= form_for(@confirmation, url: confirmation_path) do |f| %>
 <%= render "devise/shared/error_messages", resource: @confirmation %>

 <div class="field">
  <%= f.label :family_name %><br />
  <%= f.text_field :family_name %>
  </div>

  <div class="field">
  <%= f.label :last_name %><br />
  <%= f.text_field :last_name %>
  </div>

  <div class="field">
  <%= f.label :frigana_first_name %><br />
  <%= f.text_field :frigana_first_name %>
  </div>

  <div class="field">
  <%= f.label :frigana_last_name %><br />
  <%= f.text_field :frigana_last_name %>
  </div>

  <div class="field">
  <%= f.label :year %><br />
  <%= f.text_field :year %>
  </div>

  <div class="field">
  <%= f.label :month %><br />
  <%= f.text_field :month %>
  </div>

  <div class="field">
  <%= f.label :day %><br />
  <%= f.text_field :day %>
  </div>

  <div class="action">
  <%= f.submit "Sign up"%>
  </div>
  <% end %>